{
  "name": "front-admin-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "check:encoding": "powershell -ExecutionPolicy Bypass -File ./scripts/check-encoding.ps1",
    "predev": "npm run check:encoding",
    "prebuild": "npm run check:encoding",
    "prepreview": "npm run check:encoding",
    "test:unit": "vitest run",
    "e2e": "playwright test",
    "e2e:full": "set E2E_RUN_LOGOUT=1&& playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write \"src/**/*\""},
  "dependencies": {
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-vue": "^9.31.0",
    "vue-eslint-parser": "^9.4.3",
    "@typescript-eslint/parser": "^8.0.0",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.3"
  }
}
